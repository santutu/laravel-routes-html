{
    "name": "s-patompong/laravel-routes-html",
    "description": "View your Laravel routes on the browser.",
    "keywords": [
        "s-patompong",
        "laravel",
        "laravel-routes-html",
        "route-list"
    ],
    "homepage": "https://github.com/s-patompong/laravel-routes-html",
    "license": "MIT",
    "authors": [
        {
            "name": "Patompong Savaengsuk",
            "email": "sp.patompong@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.37|^9"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.10",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.4",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "SPatompong\\LaravelRoutesHtml\\": "src",
            "SPatompong\\LaravelRoutesHtml\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SPatompong\\LaravelRoutesHtml\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SPatompong\\LaravelRoutesHtml\\LaravelRoutesHtmlServiceProvider"
            ],
            "aliases": {
                "LaravelRoutesHtml": "SPatompong\\LaravelRoutesHtml\\Facades\\LaravelRoutesHtml"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
